{
  "expo": {
    "name": "Peace",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/peacelogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/peacelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "rgb(247, 247, 255)"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/peacelogo.png",
        "backgroundColor": "rgb(247, 247, 255)"
      }
    },
    "web": {
      "favicon": "./assets/peacelogo.png"
    }
  }
}
